@model IEnumerable<MedicalDb.Models.visa_category>

@{
    ViewBag.Title = "Visa_Category";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Visa Category</h2>

<p>
    @Html.ActionLink("Create New", "Create_Visa_Category")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit_Visa_Category", new { id = item.visacategory_id }) |
            @Html.ActionLink("Delete", "Delete_Visa_Category", new { id = item.visacategory_id })
        </td>
    </tr>
}

</table>
